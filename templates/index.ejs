<!doctype html>
<html lang=en-US>
<head>
  <%- include('meta') %>
  <title><%= blogTitle %></title>
</head>
<body>
  <%- include('header') %>
  <%- include('nav') %>
  <main role=main>
    <ol class=posts>
    <% for (const post of posts) { %>
      <%- include('post-li', { post }) %>
    <% } %>
    </ol>
  </main>
  <%- include('footer') %>
</body>
</html>
